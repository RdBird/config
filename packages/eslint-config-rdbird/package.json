{
  "name": "eslint-config-rdbird",
  "version": "0.0.1",
  "description": "RdBird's ESLint configuration",
  "homepage": "https://github.com/RdBird/eslint-config-rdbird#readme",
  "author": "Julien Polo <jpolo@auchan.fr>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "rules:list": "eslint-index .eslintrc.js --docs",
    "rules:table": "eslint-index .eslintrc.js --format table",
    "rules:count": "eslint-index .eslintrc.js --format number",
    "rules:list-omitted": "eslint-index .eslintrc.js --status omitted --docs",
    "rules:count-omitted": "eslint-index .eslintrc.js --status omitted --format number",
    "rules:list-enabled": "eslint-index .eslintrc.js --status warn error --docs",
    "rules:count-enabled": "eslint-index .eslintrc.js --status warn error --format number",
    "rules:list-disabled": "eslint-index .eslintrc.js --status omitted off --docs",
    "rules:count-disabled": "eslint-index .eslintrc.js --status omitted off --format number"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "rdbird"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RdBird/eslint-config-rdbird.git"
  },
  "bugs": {
    "url": "https://github.com/RdBird/eslint-config-rdbird/issues"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1"
  },
  "devDependencies": {
    "eslint-index": "^1.2.0"
  },
  "peerDependencies": {
    "eslint": "^3.13.1"
  }
}
