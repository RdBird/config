{
  "name": "@rdbird/eslint-config",
  "version": "0.0.2",
  "description": "RdBird's ESLint configuration",
  "homepage": "https://github.com/RdBird/eslint-config#readme",
  "author": "Julien Polo <jpolo@auchan.fr>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "eslint --config __tests__/eslintrc-test.js __tests__/fixtures",
    "rule:list": "eslint-index __tests__/eslintrc-test.js --docs",
    "rule:table": "eslint-index __tests__/eslintrc-test.js --format table",
    "rule:count": "eslint-index __tests__/eslintrc-test.js --format number",
    "rule:list-omitted": "eslint-index __tests__/eslintrc-test.js --status omitted --docs",
    "rule:count-omitted": "eslint-index __tests__/eslintrc-test.js --status omitted --format number",
    "rule:list-enabled": "eslint-index __tests__/eslintrc-test.js --status warn error --docs",
    "rule:count-enabled": "eslint-index __tests__/eslintrc-test.js --status warn error --format number",
    "rule:list-disabled": "eslint-index __tests__/eslintrc-test.js --status omitted off --docs",
    "rule:count-disabled": "eslint-index __tests__/eslintrc-test.js --status omitted off --format number"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "rdbird"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RdBird/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/RdBird/eslint-config/issues"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsdoc": "^2.4.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1"
  },
  "devDependencies": {
    "eslint-index": "^1.2.0"
  },
  "peerDependencies": {
    "eslint": "^3.13.1"
  }
}
